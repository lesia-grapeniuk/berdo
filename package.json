{
  "name": "berdo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:clean": "ng build --base-href=/berdo/ && cp -r dist/berdo/browser/* dist/berdo && rm -rf dist/berdo/browser",
    "deploy": "npm run build:clean && npx angular-cli-ghpages --dir=dist/berdo"
  },
